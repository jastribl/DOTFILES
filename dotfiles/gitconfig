[core]
	editor = vim
	excludesfile = ~/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[credential]
	helper = osxkeychain
[user]
	name = Justin Stribling
	useConfigOnly = true
[gui]
	fontdiff = -family Monaco -size 13 -weight normal -slant roman -underline 0 -overstrike 0
	tabsize = 4
[push]
	default = current
[pull]
	default = current
	rebase = true
[alias]
	checkoutr = checkout
[diff]
	tool = vimdiff
[interactive]
	diffFilter = diff-so-fancy --patch-mode | less --tabs=4 -RFX
[rebase]
	autoStash = true
[remote "origin"]
	prune = true
