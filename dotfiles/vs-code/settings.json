{
    "editor.fontSize": 15,
    "editor.renderWhitespace": "selection",
    "workbench.colorTheme": "Base16 Tomorrow Dark",
    "zenMode.hideLineNumbers": false,
    "multiCommand.commands": [
        {
            "command": "multiCommand.endLineInsertFromNormal",
            "sequence": [
                "cursorHome",
                "cursorEnd",
                "extension.vim_insert"
            ]
        },
        {
            "command": "multiCommand.startLineInsertFromNormal",
            "sequence": [
                "cursorHome",
                "extension.vim_insert",
            ]
        },
        {
            "command": "multiCommand.endLineInsertFromVisual",
            "sequence": [
                "extension.vim_end"
            ]
        },
        {
            "command": "multiCommand.startLineInsertFromVisual",
            "sequence": [
                "extension.vim_home"
            ]
        }
    ],
    "editor.minimap.enabled": false,
    "vim.useSystemClipboard": true,
    "vim.whichwrap": "<,>,h,l,[,]",
    "workbench.editor.enablePreview": false,
    "interactive-smartlog.pull": "hg pull",
    "interactive-smartlog.fetch": "jf get",
    "workbench.editor.tabSizing": "shrink",
    "new-file-template.oncall_name": "creative_aware_infra",
    "interactive-smartlog.right-drawer-width": 300,
    "interactive-smartlog.bottom-drawer-height": 500,
    "[cpp]": {
        "editor.defaultFormatter": "nuclide.cpp",
        "files.trimTrailingWhitespace": false
    },
    "remote.extensionKind": {
        "ryuta46.multi-command": [
            "ui"
        ],
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "fb.ms-remote-connections.odtypes": [
        {
            "type": "ondemand_www",
            "flavor": "default",
            "folders": [
                "/data/sandcastle/boxes/www/"
            ]
        },
        {
            "type": "ondemand_bento",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_dataswarm",
            "flavor": "default",
            "folders": [
                "/data/sandcastle/boxes/fbsource/"
            ]
        },
        {
            "type": "ondemand_instagram",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_configerator",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_www",
            "flavor": "releng_www",
            "folders": []
        },
        {
            "type": "ondemand_www_fbsource_configerator",
            "flavor": "default",
            "folders": []
        }
    ],
    "fb.ms-remote-connections.hosts": [],
    "blame.disableInlineBlame": true,
    "workbench.startupEditor": "none",
    "interactive-smartlog.autoRunMergeDrivers": true,
    "fb.doctor.kerberos-keychain": true,
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/bin/bash",
        },
    },
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "/bin/bash",
        },
    },
    "terminal.integrated.defaultProfile.osx": "bash",
    "wasabi.interactiveDevelopment.introMessageNumTimesShown": 10,
    "terminal.integrated.systemd.shouldUse": true,
    "editor.inlineSuggest.allowSuggestOnTriggerCharacters": true,
    "[css]": {
        "editor.defaultFormatter": "nuclide.prettier"
    },
    "meta.global.history.enabled": true,
    "interactive-smartlog.prefer-new-isl": true,
    "notebook.formatOnSave.enabled": true,
    "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
    "remora.focusOnStartup": false,
    "[python]": {
        "files.trimTrailingWhitespace": false
    },
    "[java]": {
        "files.trimTrailingWhitespace": false
    },
    "[kotlin]": {
        "files.trimTrailingWhitespace": false
    },
    "arcinfer.runOnSave": true,
    "[rust]": {
        "files.trimTrailingWhitespace": false
    },
    "workbench.editor.empty.hint": "hidden",
}
