{
    "editor.fontSize": 15,
    "editor.renderWhitespace": "selection",
    "zenMode.hideLineNumbers": false,
    "multiCommand.commands": [
        {
            "command": "multiCommand.endLineInsertFromNormal",
            "sequence": [
                "cursorHome",
                "cursorEnd",
                "extension.vim_insert"
            ]
        },
        {
            "command": "multiCommand.startLineInsertFromNormal",
            "sequence": [
                "cursorHome",
                "extension.vim_insert",
            ]
        },
        {
            "command": "multiCommand.endLineInsertFromVisual",
            "sequence": [
                "extension.vim_end"
            ]
        },
        {
            "command": "multiCommand.startLineInsertFromVisual",
            "sequence": [
                "extension.vim_home"
            ]
        }
    ],
    "editor.minimap.enabled": false,
    "vim.useSystemClipboard": true,
    "vim.whichwrap": "<,>,h,l,[,]",
    "workbench.editor.enablePreview": false,
    "interactive-smartlog.pull": "hg pull",
    "interactive-smartlog.fetch": "jf get",
    "workbench.editor.tabSizing": "shrink",
    "new-file-template.oncall_name": "creative_asset_liquidity_framework",
    "interactive-smartlog.right-drawer-width": 300,
    "interactive-smartlog.bottom-drawer-height": 500,
    "[cpp]": {
        "editor.defaultFormatter": "nuclide.arclint",
        "files.trimTrailingWhitespace": false,
        "editor.formatOnSave": true
    },
    "remote.extensionKind": {
        "ryuta46.multi-command": [
            "ui"
        ],
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "fb.ms-remote-connections.odtypes": [
        {
            "type": "ondemand_www",
            "flavor": "default",
            "folders": [
                "/data/sandcastle/boxes/www/"
            ]
        },
        {
            "type": "ondemand_bento",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_dataswarm",
            "flavor": "default",
            "folders": [
                "/data/sandcastle/boxes/fbsource/"
            ]
        },
        {
            "type": "ondemand_instagram",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_configerator",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_www",
            "flavor": "releng_www",
            "folders": []
        },
        {
            "type": "ondemand_www_fbsource_configerator",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_fbcode",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_www_fbsource_configerator",
            "flavor": "confucius",
            "folders": []
        },
        {
            "type": "ondemand_fbsource",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_xplat_react",
            "flavor": "default",
            "folders": []
        }
    ],
    "fb.ms-remote-connections.hosts": [],
    "blame.disableInlineBlame": false,
    "workbench.startupEditor": "none",
    "interactive-smartlog.autoRunMergeDrivers": true,
    "fb.doctor.kerberos-keychain": true,
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/bin/bash",
        },
    },
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "/bin/bash",
        },
    },
    "terminal.integrated.defaultProfile.osx": "bash",
    "wasabi.interactiveDevelopment.introMessageNumTimesShown": 10,
    "terminal.integrated.systemd.shouldUse": true,
    "editor.inlineSuggest.allowSuggestOnTriggerCharacters": true,
    "[css]": {
        "editor.defaultFormatter": "nuclide.prettier"
    },
    "meta.global.history.enabled": true,
    "interactive-smartlog.prefer-new-isl": true,
    "notebook.formatOnSave.enabled": true,
    "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
    "remora.focusOnStartup": false,
    "[python]": {
        "files.trimTrailingWhitespace": false,
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "[java]": {
        "files.trimTrailingWhitespace": false,
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "[kotlin]": {
        "files.trimTrailingWhitespace": false,
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "arcinfer.runOnSave": true,
    "[rust]": {
        "files.trimTrailingWhitespace": false,
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "workbench.editor.empty.hint": "hidden",
    "extensions.experimental.deferredStartupFinishedActivation": true,
    "[c]": {
        "files.trimTrailingWhitespace": false,
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "[proto]": {
        "files.trimTrailingWhitespace": false,
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "[starlark]": {
        "files.trimTrailingWhitespace": false,
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "[hack]": {
        "editor.defaultFormatter": "nuclide.hack"
    },
    "[objective-c]": {
        "editor.defaultFormatter": "nuclide.cpp",
        "editor.formatOnSave": true
    },
    "[objective-cpp]": {
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "[cuda-cpp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "nuclide.arclint"
    },
    "[tcc-cpp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "nuclide.arclint"
    },
    "[ocaml]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "nuclide.arclint"
    },
    "sapling.isl.openBeside": false,
    "editor.lightbulb.enabled": "on",
    "[swift]": {
        "editor.defaultFormatter": "nuclide.arclint",
        "editor.formatOnSave": true
    },
    "git.blame.editorDecoration.enabled": true,
    "[blueprint]": {
        "editor.defaultFormatter": "nuclide.arclint"
    },
    "workbench.colorTheme": "Tomorrow Night",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "ctrl+e"
            ],
            "after": [
                "S-A"
            ]
        },
        {
            "before": [
                "ctrl+a"
            ],
            "after": [
                "S-I"
            ]
        }
    ],
    "[go]": {
        "editor.defaultFormatter": "nuclide.go"
    },
    "[starlark-skycastle]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "nuclide.arclint"
    },
    "devmate.clickableSparkleDecorator": false,
    "accessibility.openChatEditedFiles": false,
    "devmate.inline.assertiveness.enable-inline-buttons-highlighted-text": false,
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "emmet.showAbbreviationSuggestions": false,
    "emmet.showExpandedAbbreviation": "never",
    "window.title": "${devmate:status}${separator}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
}
