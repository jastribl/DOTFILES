{
    "window.zoomLevel": 0,
    "editor.fontSize": 15,
    "editor.renderWhitespace": "selection",
    "workbench.colorTheme": "Base16 Tomorrow Dark",
    "oneDarkPro.editorTheme": "oneDarkPro",
    "zenMode.hideLineNumbers": false,
    "multiCommand.commands": [
        {
            "command": "multiCommand.endLineInsertFromNormal",
            "sequence": [
                "cursorLineEnd",
                "extension.vim_insert",
            ]
        },
        {
            "command": "multiCommand.startLineInsertFromNormal",
            "sequence": [
                "cursorHome",
                "extension.vim_insert",
            ]
        },
        {
            "command": "multiCommand.endLineInsertFromVisual",
            "sequence": [
                "extension.vim_end"
            ]
        },
        {
            "command": "multiCommand.startLineInsertFromVisual",
            "sequence": [
                "extension.vim_home"
            ]
        }
    ],
    "zenMode.hideTabs": false,
    "editor.minimap.enabled": false,
    "vim.useSystemClipboard": true,
    "vim.whichwrap": "<,>,h,l,[,]",
    "workbench.editor.enablePreview": false,
    "workbench.activityBar.visible": true,
    "workbench.editor.tabSizing": "shrink",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "go.formatTool": "goimports"
}
