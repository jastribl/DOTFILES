// Place your key bindings in this file to override the defaults
[
    {
        "key": "cmd+[",
        "command": "-editor.action.outdentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+[",
        "command": "cursorUndo",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+u",
        "command": "-cursorUndo",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+]",
        "command": "cursorRedo"
    },
    {
        "key": "cmd+]",
        "command": "-editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+a",
        "command": "-cursorLineStart",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+a",
        "command": "-extension.vim_ctrl+a",
        "when": "editorTextFocus && vim.active && vim.use<C-a> && !inDebugRepl"
    },
    {
        "key": "ctrl+e",
        "command": "-cursorLineEnd",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+e",
        "command": "-extension.vim_ctrl+e",
        "when": "editorTextFocus && vim.active && vim.use<C-e> && !inDebugRepl"
    },
    {
        "key": "ctrl+e",
        "command": "multiCommand.endLineInsertFromNormal",
        "when": "textInputFocus && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+a",
        "command": "multiCommand.startLineInsertFromNormal",
        "when": "textInputFocus && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+e",
        "command": "multiCommand.endLineInsertFromVisual",
        "when": "textInputFocus && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "ctrl+a",
        "command": "multiCommand.startLineInsertFromVisual",
        "when": "textInputFocus && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "ctrl+e",
        "command": "cursorLineEnd",
        "when": "textInputFocus && vim.active && vim.mode == 'Insert'"
    },
    {
        "key": "ctrl+a",
        "command": "cursorHome",
        "when": "textInputFocus && vim.active && vim.mode == 'Insert'"
    },
    {
        "key": "home",
        "command": "-cursorHome",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+r cmd+r",
        "command": "editor.action.rename",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "f2",
        "command": "-editor.action.rename",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+f12",
        "command": "-editor.action.goToReferences",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
        "key": "shift+f12",
        "command": "-goToPreviousReferenceFromEmbeddedEditor",
        "when": "inReferenceSearchEditor"
    },
    {
        "key": "shift+f12",
        "command": "-goToPreviousReference",
        "when": "referenceSearchVisible"
    },
    {
        "key": "shift+f12",
        "command": "references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "shift+alt+f12",
        "command": "-references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.navigateUp",
        "when": "editorTextFocus && vim.active && !inDebugRepl"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.navigateDown",
        "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode != 'Visual'"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.navigateLeft",
        "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode != 'Visual'"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.navigateRight",
        "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode != 'Visual'"
    },
    {
        "key": "\\ \\",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly && vim.active && vim.mode == 'Normal' || vim.mode == 'Visual' || editorHasSelection"
    },
    {
        "key": "ctrl+j",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+l",
        "command": "editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "ctrl+h",
        "command": "editor.action.outdentLines",
        "when": "editorTextFocus && !editorReadonly && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "ctrl+o",
        "command": "cursorUndo",
        "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+i",
        "command": "cursorRedo",
        "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode == 'Normal'"
    },
    {
        "key": "cmd+k z",
        "command": "workbench.action.toggleZenMode"
    },
    {
        "key": "ctrl+shift+d",
        "command": "workbench.action.toggleZenMode"
    },
    {
        "key": "cmd+shift+c",
        "command": "editor.action.formatSelection",
        "when": "editorHasDocumentSelectionFormattingProvider && editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly && editorHasSelection"
    },
    {
        "key": "cmd+shift+c",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentSelectionFormattingProvider && editorHasDocumentSelectionFormattingProvider && !editorReadonly && !editorHasSelection"
    },
    {
        "key": "] c",
        "command": "workbench.action.editor.nextChange",
        "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+f5",
        "command": "-workbench.action.editor.nextChange",
        "when": "editorTextFocus"
    },
    {
        "key": "[ c",
        "command": "workbench.action.editor.previousChange",
        "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+f5",
        "command": "-workbench.action.editor.previousChange",
        "when": "editorTextFocus"
    },
    {
        "key": "f8",
        "command": "workbench.action.debug.continue",
        "when": "inDebugMode"
    },
    {
        "key": "f5",
        "command": "-workbench.action.debug.continue",
        "when": "inDebugMode"
    },
    {
        "key": "shift+cmd+\\",
        "command": "workbench.files.action.showActiveFileInExplorer"
    },
    {
        "key": "shift+cmd+\\",
        "command": "-editor.action.jumpToBracket",
        "when": "editorTextFocus"
    },
    {
        "key": "] e",
        "command": "editor.action.marker.next",
        "when": "textInputFocus && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "alt+f8",
        "command": "-editor.action.marker.next",
        "when": "editorFocus"
    },
    {
        "key": "[ e",
        "command": "editor.action.marker.prev",
        "when": "textInputFocus && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "shift+alt+f8",
        "command": "-editor.action.marker.prev",
        "when": "editorFocus"
    },
    {
        "key": "shift+cmd+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    }
]
